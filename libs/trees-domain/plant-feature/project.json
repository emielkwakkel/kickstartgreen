{
  "projectType": "library",
  "root": "libs/trees-domain/plant-feature",
  "sourceRoot": "libs/trees-domain/plant-feature/src",
  "prefix": "plant",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/trees-domain/plant-feature"],
      "options": {
        "jestConfig": "libs/trees-domain/plant-feature/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/trees-domain/plant-feature/src/**/*.ts",
          "libs/trees-domain/plant-feature/src/**/*.html"
        ]
      }
    }
  },
  "tags": ["domain:trees", "type:feature"]
}
